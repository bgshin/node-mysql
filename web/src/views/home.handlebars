<!DOCTYPE html>
<html>
<head>
	<title>Demo Page</title>
</head>
<body>

	<h2>{{title}}</h2>

	<!-- <h2>{{results}}</h2> -->

	<table>
		<thead>
			<th>First Name</th>
			<th>Last Name</th>
			<th>Age</th>
		</thead>

		<tbody>
			{{#each results}}
			<tr>
				<td>{{this.first_name}}</td>
				<td>{{this.last_name}}</td>
				<td>{{this.age}}</td>
			</tr>
			{{/each}}
		</tbody>


	</table>

</body>
</html>